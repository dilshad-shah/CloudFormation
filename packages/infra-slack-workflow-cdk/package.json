{
  "name": "@cvent/infra-slack-workflow-cdk",
  "version": "1.9.0",
  "license": "UNLICENSED",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build:ts": "tsc",
    "build:cdk": "cdk-cvent build",
    "build": "run-s -ls build:*",
    "lint:prettier": "prettier-cvent lint",
    "lint:eslint": "eslint . --max-warnings 0",
    "lint": "run-p -ls lint:*",
    "test:ts": "jest",
    "test:cdk": "cdk-cvent test",
    "test": "run-p -ls test:*",
    "fix:eslint": "eslint . --fix",
    "fix:prettier": "prettier-cvent fix",
    "fix": "run-s -ls fix:*",
    "ci:setup": "cdk-cvent deploy",
    "ci:teardown": "cdk-cvent destroy",
    "release:publish": "cdk-cvent release:publish --project infra-slack-workflow",
    "clean": "rm -rf cdk.out dist",
    "updatesnapshot": "jest --update-snapshot"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.193.0",
    "@aws-cdk/aws-dynamodb": "^1.193.0",
    "@aws-cdk/aws-iam": "^1.193.0",
    "@aws-cdk/aws-lambda": "^1.193.0",
    "@aws-cdk/core": "^1.183.0",
    "@cvent/aws-cdk-core": "^1.121.0",
    "@cvent/environments": "^1.10.3",
    "@cvent/infra-slack-workflow-lambda": "workspace:^0.11.0",
    "@cvent/octopusdeploy-cdk": "3.3.2",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.183.0",
    "@cvent/aws-cdk-credential-plugin": "^1.121.0",
    "@cvent/builder-cdk": "^2.7.2",
    "@cvent/builder-prettier": "^1.1.3",
    "@cvent/nucleus-eslint": "^3.2.3",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.18.5",
    "aws-cdk": "^1.183.0",
    "eslint": "^7.31.0",
    "jest": "^25.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "ts-jest": "^25.5.1",
    "typescript": "^4.5.4"
  }
}
